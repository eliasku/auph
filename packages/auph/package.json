  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/eliasku/auph.git",
    "directory": "packages/native"
  },
  "devDependencies": {
    "typescript": "^4.3.5",
    "rollup": "^2.53.2",
    "rollup-plugin-terser": "^7.0.2",
    "@rollup/plugin-replace": "^2.4.2"
  },
  "dependencies": {
    "bindings": "^1.5.0",
    "node-addon-api": "^4.0.0"
  },
  "gypfile": true,
  "engines": {
    "node": "~10 >=10.20 || >=12.17"
  }
}
