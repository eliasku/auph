{
  "name": "auph",
  "version": "0.0.3",
  "browser": "web/dist/browser/auph.js",
  "main": "web/dist/cjs/index.js",
  "module": "web/dist/module/index.js",
  "types": "web/dist/types/index.d.ts",
  "scripts": {
    "watch": "tsc -w -p web/tsconfig.json & rollup -w -c web/rollup.config.js",
    "build": "tsc -p web/tsconfig.json && rollup -c web/rollup.config.js",
    "build-cmake": "node scripts/build-cmake.js",
    "build-node": "node-gyp rebuild",
    "postbuild": "node scripts/build-demo && node scripts/build-pages",
    "demo-node": "node demo/node.js",
    "release": "changeset publish",
    "version": "changeset version && yarn run update-lock",
    "update-lock": "yarn && git add yarn.lock"
  },
  "files": [
    "src",
    "external",
    "android",
    "nodejs",
    "CMakeLists.txt",
    "ek.js",
    "binding.gyp",
    "web/dist",
    "README.md",
    "REFERENCES.md",
    "CHANGELOG.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/eliasku/auph.git"
  },
  "devDependencies": {
    "typescript": "^4.3.5",
    "rollup": "^2.53.2",
    "rollup-plugin-terser": "^7.0.2",
    "@rollup/plugin-replace": "^2.4.2",
    "@changesets/cli": "^2.16.0",
    "wsrun": "^5.2.4"
  },
  "dependencies": {
    "bindings": "^1.5.0",
    "node-addon-api": "^4.0.0"
  },
  "gypfile": true,
  "engines": {
    "node": "~10 >=10.20 || >=12.17"
  }
}
