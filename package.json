{
  "name": "auph",
  "version": "0.0.15",
  "browser": "web/dist/browser/auph.js",
  "main": "web/dist/cjs/index.js",
  "module": "web/dist/module/index.js",
  "types": "web/dist/types/index.d.ts",
  "scripts": {
    "install": "npx npm-cmake || exit 0",
    "check1": "node -p \"require('./node-include.js')('@ekx/stb')\"",
    "check2": "node -p \"require('./node-include.js')('@ekx/dr-libs')\"",
    "postinstall": "node-gyp configure && node-gyp build",
    "watch": "tsc -w -p web/tsconfig.json & rollup -w -c web/rollup.config.js",
    "build": "tsc -p web/tsconfig.json && rollup -c web/rollup.config.js",
    "build-cmake": "node scripts/build-cmake",
    "build-demo": "node scripts/build-demo",
    "build-pages": "node scripts/build-demo && node scripts/build-pages",
    "build-node": "node-gyp rebuild",
    "start-node": "cd demo && node node.js",
    "update-cpp-deps": "node scripts/update-cpp-deps.js",
    "release": "changeset publish",
    "version": "changeset version && yarn run update-lock",
    "update-lock": "yarn && git add yarn.lock",
    "clean": "rimraf ./build ./pages ./web/dist"
  },
  "files": [
    "include",
    "android",
    "CMakeLists.txt",
    "auph-static.cpp",
    "ek.js",
    "nodejs",
    "binding.gyp",
    "web/dist",
    "README.md",
    "REFERENCES.md",
    "CHANGELOG.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/eliasku/auph.git"
  },
  "devDependencies": {
    "@changesets/cli": "^2.16.0",
    "@rollup/plugin-replace": "^3.0.0",
    "follow-redirects": "^1.14.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.56.3",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "bindings": "^1.5.0",
    "node-addon-api": "^4.0.0",
    "npm-cmake": "^0.1.0",
    "@ekx/stb": "^0.0.6",
    "@ekx/dr-libs": "^0.0.2"
  },
  "gypfile": true,
  "engines": {
    "node": "~10 >=10.20 || >=12.17"
  },
  "keywords": [
    "auph",
    "auph.js",
    "audio",
    "sound",
    "web audio",
    "webaudio",
    "c++",
    "nodejs",
    "coreaudio",
    "oboe",
    "aaudio",
    "android",
    "ios",
    "opensles",
    "xcode",
    "audiosession",
    "macos",
    "native",
    "audio mixer"
  ],
  "homepage": "https://eliasku.github.io/auph"
}
